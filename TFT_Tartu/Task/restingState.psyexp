EG = expInfo[&quot;EEG&quot;];&amp;#10;function sendTrigger(t, trigN, EEG) {&amp;#10;    if ((EEG === &quot;1&quot;)) {&amp;#10;        if (((t &lt; 0.05) &amp;&amp; (t &gt; 0))) {&amp;#10;            port.setData(Number.parseInt(trigN, 2));&amp;#10;        } else {&amp;#10;            port.setData(0);&amp;#10;        }&amp;#10;    }&amp;#10;}&amp;#10;" valType="extendedCode"/>
        <Param name="Begin JS Routine" updates="constant" val="&amp;#10;        // add-on: list(s: string): string[]&amp;#10;        function list(s) {&amp;#10;            // if s is a string, we return a list of its characters&amp;#10;            if (typeof s === 'string')&amp;#10;                return s.split('');&amp;#10;            else&amp;#10;                // otherwise we return s:&amp;#10;                return s;&amp;#10;        }&amp;#10;&amp;#10;        n = util.randint(1, 5);&amp;#10;randTime = list(util.randint(1, Number.parseInt(expInfo[&quot;rest_dur&quot;]), n));&amp;#10;indx = 0;&amp;#10;trigNStart = &quot;11000000&quot;;&amp;#10;trigNChange = &quot;11110000&quot;;&amp;#10;offCOl = true;&amp;#10;" valType="extendedCode"/>
        <Param name="Begin Routine" updates="constant" val="&amp;#10;n = randint(1,5)&amp;#10;randTime = list(randint(1,int(expInfo['rest_dur']), n))&amp;#10;indx = 0&amp;#10;trigNStart = '11000000'&amp;#10;trigNChange = '11110000'&amp;#10;&amp;#10;offCOl = True" valType="extendedCode"/>
        <Param name="Code Type" updates="None" val="Auto-&gt;JS" valType="str"/>
        <Param name="Each Frame" updates="constant" val="sendTrigger(t, trigNStart, EEG)&amp;#10;gabors_large.draw()&amp;#10;#circle.draw()&amp;#10;#gabors_small.draw()&amp;#10;#&amp;#10;#if any(t - randTime) &lt; 0.25 and any(t - randTime) &gt; 0:&amp;#10;#    fixCol = [0.75,0.75,0.75]&amp;#10;#else:&amp;#10;#    fixCol =  [0.25,0.25,0.25]&amp;#10;&amp;#10;if True in (abs(t - ri) &lt; 0.25 for ri in randTime): # and True in (t - ri &gt; 0 for ri in randTime):&amp;#10;    if offCOl:&amp;#10;        cTime = t&amp;#10;    sendTrigger(t-cTime, trigNChange, EEG)&amp;#10;    fixCol = [0.75,0.75,0.75]&amp;#10;else:&amp;#10;    offCOl = True&amp;#10;    fixCol =  [0.25,0.25,0.25]" valType="extendedCode"/>
        <Param name="Each JS Frame" updates="constant" val="/* Syntax Error: Fix Python code */" valType="extendedCode"/>
